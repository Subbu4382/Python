    "#Generate the Fibonacci sequence up to n terms.\n",
    "a,b=0,1\n",
    "n=int(input())\n",
    "for i in range(n):\n",
    "    c=a+b\n",
    "    a=b\n",
    "    b=c\n",
    "    print(c,end=\" \")"
